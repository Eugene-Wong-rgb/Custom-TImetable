from tkinter import *

import tkinter as tk

tk = Tk()

tk.title('Time Manager')
tk.geometry("500x500")

frame1=Frame(tk)
frame1.pack(side=TOP,fill=X)

frame2=Frame(tk)
frame2.pack(side=TOP, fill=X)

class menu(tk.Frame):
     def __init__(self, master):
        tk.Frame.__init__(frame1)
        tk.Frame.pack(side=TOP,fill=X)
        tk.Label(frame1, text = "Welcome to Time Manager")
        create_timetable=Frame(frame1)
        create_timetable.pack(side=TOP,fill=X)
        create_timetable = Button(frame1, text = "Create Custom Timetable", command = custom_table)
        create_timetable.pack()




class custom_timetable(Tk.Frame):
    def __init__(self, master):
        tk.Frame.__init__(self, master)
        tk.Button(self, text="").grid(row=0, column=0, sticky ='ew')
        tk.Button(frame2, text="Monday").grid(row=0, column=1 , sticky ='ew')
        tk.Button(frame2, text="Tuesday").grid(row=0, column=2 , sticky ='ew')
        tk.Button(frame2, text="Wednesday").grid(row=0, column=3 , sticky ='ew')
        tk.Button(frame2, text="Thursday").grid(row=0, column=4 , sticky ='ew')
        tk.Button(frame2, text="Friday").grid(row=0, column=5 , sticky ='ew')
        tk.Button(frame2, text="Saturday").grid(row=0, column=6 , sticky ='ew')
