        tk.Button(frame2, text="2PM-3PM").grid(row=7, column=0, sticky='ew')
        tk.Button(frame2, text="3PM-4PM").grid(row=8, column=0, sticky='ew')
        tk.Button(frame2, text="4PM-5PM").grid(row=9, column=0, sticky='ew')


        tk.Entry(frame2).grid(row=1, column=1, sticky='ew')
